<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Characters</title>
</head>
<body>
    <h1>Characters</h1>
    <ul>
        <% characters.forEach(function(character) { %>
            <li>
                <img src="<%= character.img %>" alt="<%= character.name %>">
                <p><%= character.name %></p>
                <!-- Add more character details as needed -->
            </li>
        <% }) %>
    </ul>
</body>
</html>
