<head>
    <%- include('./partials/head.ejs', {title: 'Index page'}) %>
    <link rel="stylesheet" href="css/styles.css">
  </head>
  
    <body>
      <nav>
        <%- include('./partials/nav.ejs') %>
      </nav>
      <!-- <h1 class="kiosk-computer-font-06">Citizen. You are viewing the Index Page.</h1> -->
     <ul> 
          <% citizens.forEach((item, index) => { %>
          <li>
              <a href="/citizens/<%=item._id %>"><%= item.name %></a>
  
              <form action="/citizens/<%= item._id %>?_method=DELETE" method="POST">
                <input type="submit" value="DELETE">
              </form>
          </li>
          <% }) %>
      </ul>
      <%- include('./partials/footer.ejs') %>
      <script src="/js/app.js"></script>
    </body>
  </html>
  